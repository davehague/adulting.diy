# Database
DATABASE_URL=your_database_url_here

# Google OAuth
NUXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here

# JWT Secret for Email Authentication
# Generate a secure random string (e.g., using: openssl rand -base64 32)
JWT_SECRET=your_secure_jwt_secret_here

# Mailjet Email Service
MAILJET_API_KEY=your_mailjet_api_key_here
MAILJET_SECRET_KEY=your_mailjet_secret_key_here
MAILJET_FROM_EMAIL=your_from_email_here
MAILJET_FROM_NAME=your_from_name_here

# Development Login Bypass (set to true only in development)
DEV_LOGIN_BYPASS=false

# Node Environment
NODE_ENV=development
